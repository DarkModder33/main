    "@types/three": "^0.182.0",
    "autoprefixer": "^10.4.24",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "postcss": "^8",
    "rimraf": "^5.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
