{ 
    \"version\": \"0.2.0\", 
    \"configurations\": [ 
        { 
            \"name\": \"Next.js: Debug Server\", 
            \"type\": \"node-terminal\", 
            \"request\": \"launch\", 
            \"command\": \"npm run dev\", 
            \"serverReadyAction\": { 
                \"pattern\": \"- Local:\\s+(https?://\\S+)\", 
                \"uriFormat\": \"%%s\", 
                \"action\": \"debugWithChrome\" 
            } 
        }, 
        { 
            \"name\": \"Next.js: Debug Client\", 
            \"type\": \"chrome\", 
            \"request\": \"launch\", 
            \"url\": \"http://localhost:3000\", 
            \"webRoot\": \"${workspaceFolder}\", 
            \"sourceMaps\": true 
        }, 
        { 
            \"name\": \"Next.js: Build\", 
            \"type\": \"node-terminal\", 
            \"request\": \"launch\", 
            \"command\": \"npm run build\" 
        } 
    ] 
}
